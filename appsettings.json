{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Password=${DB_PASSWORD};Username=${DB_USER};Database=${DB_NAME};Integrated Security=false;Persist Security Info=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "UsePostgres": true,
  "UseMySql": false,

  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      }
    }
  }
}